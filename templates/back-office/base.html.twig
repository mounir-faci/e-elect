{% trans_default_domain 'translations' %}

<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>
            {% block title %}
                {{ 'back.title' | trans }}
            {% endblock %}
        </title>

        {# stylesheets #}
        <link href="{{ asset('assets/back-office/vendor/fontawesome-free/css/all.min.css') }}" rel="stylesheet" type="text/css">
        <link href="{{ asset('assets/back-office/vendor/bootstrap4-toggle/css/bootstrap4-toggle.min.css')}}" rel="stylesheet">
        <link href="{{ asset('assets/back-office/vendor/datatables/dataTables.bootstrap4.min.css')}}" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
        <link href="{{ asset('assets/back-office/css/sb-admin-2.min.css') }}" rel="stylesheet">

        {% block stylesheets %}

        {% endblock %}
    </head>
    <body id="page-top" class="{% block bodyClass %} {% endblock %}">
        {% block body %}

        {% endblock %}

        {# javascripts #}
        <script src="{{ asset('assets/back-office/vendor/jquery/jquery.min.js') }}"></script>
        <script src="{{ asset('assets/back-office/vendor/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
        <script src="{{ asset('assets/back-office/vendor/jquery-easing/jquery.easing.min.js') }}"></script>
        <script src="{{ asset('assets/back-office/js/sb-admin-2.min.js') }}"></script>
        <script src="{{ asset('assets/back-office/vendor/chart.js/Chart.min.js') }}"></script>
        <script src="{{ asset('assets/back-office/vendor/bootstrap4-toggle/js/bootstrap4-toggle.min.js')}}"></script>
        <script src="{{ asset('assets/back-office/vendor/datatables/jquery.dataTables.min.js')}}"></script>
        <script src="{{ asset('assets/back-office/vendor/datatables/dataTables.bootstrap4.min.js')}}"></script>

        {% block javascripts %}

        {% endblock %}
    </body>
</html>
